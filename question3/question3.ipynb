   "source": [
    "Define Bessel function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "def J(m,x):\n",
    "    integrand = lambda theta: 1/(np.pi)*np.cos(m*theta - x*np.sin(theta))\n",
    "    result,err = integrate.quad(integrand,0, np.pi)\n",
    "    return result"
   ]
