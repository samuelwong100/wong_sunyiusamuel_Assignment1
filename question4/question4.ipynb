   "source": [
    "import numpy as np\n",
    "dx =  10**(-2)\n",
    "dy = 10**(-2)\n",
    "x = np.arange(-2+dx,2,dx)\n",
    "y = np.arange(-2+dy,2,dy)\n",
    "z = x + (0+1j)*y"
